	# START OF flamework-elasticsearch STUFF

	$GLOBALS['cfg']['elasticsearch_host'] = 'localhost';
	$GLOBALS['cfg']['elasticsearch_port'] = '9200';
	$GLOBALS['cfg']['elasticsearch_http_timeout'] = 10;

	# https://www.elastic.co/guide/en/elasticsearch/guide/current/scroll.html

	$GLOBALS['cfg']['elasticsearch_scroll'] = 1;
	$GLOBALS['cfg']['elasticsearch_scroll_ttl'] = '1m';
	$GLOBALS['cfg']['elasticsearch_scroll_trigger'] = 10000;

	# END OF flamework-elasticsearch STUFF